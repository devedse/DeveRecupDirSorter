<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>DeveRecupDirSorter</AssemblyName>
    <PackageId>DeveRecupDirSorter</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <Version>1.0.0.0</Version>
    <Title>DeveRecupDirSorter</Title>
    <Authors>Devedse</Authors>
    <Description>A tool which sorts the files from TestDisk.</Description>
    <PackageProjectUrl>https://github.com/devedse/DeveRecupDirSorter</PackageProjectUrl>
    <PackageTags>csharp;recupdir;testdisk;sorting</PackageTags>
    <DebugType>full</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DeveCoolLib" Version="1.0.11" />
    <PackageReference Include="ExifLibNet" Version="1.2.34" />
  </ItemGroup>

</Project>
